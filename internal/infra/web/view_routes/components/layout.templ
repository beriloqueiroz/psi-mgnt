package components

templ Layout(children ...templ.Component) {
	<!DOCTYPE html>
	<html lang="pt=br">
		<head>
            <meta charset="UTF-8"/>
            <meta name="viewport" content="width=device-width, initial-scale=1"/>
            <title>Gerenciador</title>
            <link href="/static/css/tailwind.css" rel="stylesheet"/>
		</head>
		<header>
		    menu
		</header>
		<body>
			for _, child := range children {
				@child
			}
		</body>
		<footer>
		    footer
		</footer>
	</html>
}